    			<%=f.input :last_name %>
			</div>
			
			<div class = 'span3'>
				<%=f.input :tags %>
			</div>
			
			<div class = 'span3'>
				<%=f.input :assigned_to %>
			</div>
			
	</div>
	
	<div class = 'row'>
		<ul>  </ul>
		<ul>  </ul>
	</div>
	
	<div class = 'row'>
		
			<div class = 'span3'>
				<%=f.input :title %>
			</div>
			
			<div class = 'span3'>
				<%=f.input :email %>
			</div>
			
			<div class = 'span3'>
				 <%=f.input :phone %>
			</div>
			
			<div class = 'span3'>
				<%=f.input :mobile_phone %>
			</div>
			
		
	</div>
	
	<div class = 'row'>
		<ul>  </ul>
		<ul>  </ul>
